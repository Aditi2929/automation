		moret1.click();
		Dimension dimension11 = driver.manage().window().getSize();
		 int start_x11 = (int) (dimension11.width *0.5);
		 int start_y11 = (int) (dimension11.height *0.97);
		
		 int end_x11 = (int) (dimension11.width *0.5);
		 int end_y11 = (int) (dimension11.height *0.97);
		 TouchAction touch11 = new TouchAction(driver);
		touch11.press(ElementOption.point(start_x11, start_y11))
		.waitAction(WaitOptions.waitOptions(Duration.ofSeconds(1)))
		.moveTo(PointOption.point(end_x11,end_y11)).release().perform();
		
		
		System.out.println("Payment done with cash");
		MobileElement Emailcheck = driver.findElement(By.id("com.cravrr.ssmpos:id/cbEmail"));
		Emailcheck.click();
		
		MobileElement closeorder = driver.findElement(By.id("com.cravrr.ssmpos:id/btnOrderClose"));
		closeorder.click();
		
		MobileElement skip2 = driver.findElement(By.id("com.cravrr.ssmpos:id/btn_skip"));
		skip2.click();

		
		
		 }	
	
