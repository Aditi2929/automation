		touch.press(ElementOption.point(start_x, start_y))
		.waitAction(WaitOptions.waitOptions(Duration.ofSeconds(1)))
		.moveTo(PointOption.point(end_x,end_y)).release().perform();
		
		

		System.out.println("Payment done with cash");
	}
